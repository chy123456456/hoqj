<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="springCache">
  <diskStore path="java.io.tmpdir/spring-ehcache" />
  <defaultCache maxElementsInMemory="10000" eternal="false" overflowToDisk="true" timeToIdleSeconds="500"
    timeToLiveSeconds="1000" diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />
  <cache name="springCache" maxElementsInMemory="1024" eternal="false" timeToIdleSeconds="43200"
    timeToLiveSeconds="86400" overflowToDisk="true" />
    
  <cache name="productAndChannel" maxElementsInMemory="1024" eternal="false" timeToIdleSeconds="120"
    timeToLiveSeconds="300" overflowToDisk="true" />

</ehcache>